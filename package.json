{
  "name": "@kennyromanov/paris",
  "version": "1.0.4",
  "description": "A Module Federating Tool (Alpha)",
  "keywords": ["microfrontends", "module federation", "module federation on vue", "alpha version"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build:types": "tsc && tsc-alias",
    "build": "bin/build && npm run build:types",
    "prepare": "npm run build"
  },
  "files": [
    "dist/"
  ],
  "author": "Kenny Romanov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/node": "^24.9.2",
    "esbuild": "^0.25.11",
    "esbuild-plugin-import-glob": "^0.1.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "mitt": "^3.0.1"
  }
}
